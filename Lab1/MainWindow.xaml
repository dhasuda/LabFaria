<Window x:Class="Lab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="293">
    <Grid>
        <Button x:Name="button" Content="Calcular" HorizontalAlignment="Left" Margin="200,180,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <TextBox x:Name="ImpedanciaText" HorizontalAlignment="Left" Height="23" Margin="155,28,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="23" Margin="155,74,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox_Copy1" HorizontalAlignment="Left" Height="23" Margin="155,128,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="impedancia carga" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.632,1.538" Margin="0,25,0,0"/>
        <Label x:Name="label_Copy" Content="Impedancia intrinseca" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.632,1.538" Margin="0,71,0,0"/>
        <Label x:Name="label_Copy1" Content="Frequencia" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.632,1.538" Margin="0,125,0,0"/>
        <Label x:Name="label_Copy2" Content="Distancia" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.632,1.538" Margin="0,219,0,0"/>
        <Label x:Name="label_Copy3" Content="Comprimento" HorizontalAlignment="Left" VerticalAlignment="Top" RenderTransformOrigin="0.632,1.538" Margin="0,263,0,0"/>

    </Grid>
</Window>
